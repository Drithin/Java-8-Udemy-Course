package com.Java8UdemyCourse.forLoop;

public class findPrimes {
	
	public static void isPrime (int n ){
		
		if (n==1){
			System.out.println(n);
		}
		
		if( n >= 2){
			if(n%n==0 && n%1==0){
				System.out.println(n + " is a prime number");
				
			}
		}
	}

	public static void main(String[] args) {
		
	}

}
