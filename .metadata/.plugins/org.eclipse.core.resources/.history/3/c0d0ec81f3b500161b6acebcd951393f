package com.Java8UdemyCourse.ChallengeExercise;

public class Room {
	
	public int lights;
	public int windows;
	public int tables;
	private String cupboard;
	/**
	 * @param lights
	 * @param windows
	 * @param tables
	 * @param cupboard
	 */
	public Room(int lights, int windows, int tables, String cupboard) {
		
		this.lights = lights;
		this.windows = windows;
		this.tables = tables;
		this.cupboard = cupboard;
	}
	public int getLights() {
		return lights;
	}
	public int getWindows() {
		return windows;
	}
	public int getTables() {
		return tables;
	}
	public String getCupboard() {
		return cupboard;
	}
	
	
	
	

}
